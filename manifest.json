{
  "manifest_version": 2,

  "name": "音乐台",
  "description": "自用音乐台，从国内音乐网站爬音频，然后播放。",
  "version": "1.0",
  "icons": {
    "128": "imgs/logo.png",
    "16": "imgs/logo_16.png",
    "48": "imgs/logo_48.png"
  },

  "browser_action": {
    "default_icon": "imgs/logo2.png",
    "default_popup": "popup.html",
    "default_title": "音乐台"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
